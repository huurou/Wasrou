<Project>
  <PropertyGroup>
    <BaseIntermediateOutputPath>$(SolutionDir)\bin\obj\$(MSBuildProjectName)\</BaseIntermediateOutputPath>
    <OutputPath>$(SolutionDir)\bin\$(Configuration)\</OutputPath>
